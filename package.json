  "scripts": {
    "start": "node server/index.js",
    "magic": "npm run react-dev & npm run server-dev & npm run sass-npm-dev",
    "react-dev": "webpack -d --watch",
    "server-dev": "nodemon server/index.js",
    "sass-gem-dev": "sass --watch react/dist/sass/styles.scss:react/dist/styles.css --style compressed",
    "sass-npm-dev": "node-sass --watch react/dist/sass/styles.scss -o react/dist --output-style compressed"
  },
