    "body-parser": "^1.17.2",
    "express": "^4.15.0",
    "jquery": "^3.3.1",
    "mysql": "^2.13.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
